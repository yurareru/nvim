snippet dsu "DSU"
    struct DSU {
        vector<int> e;
        void init(int n) { e = vector<int>(n, -1); }
        int find(int x) { return (e[x] < 0 ? x : e[x] = find(e[x])); }
        int size(int x) { return -e[find(x)]; }
        bool same(int x, int y) { return find(x) == find(y); }
        bool unite(int x, int y) {
            x = find(x);
            y = find(y);
            if (x == y) return 0;
            if (e[x] > e[y]) swap(x, y);
            e[x] += e[y];
            e[y] = x;
            return 1;
        }
    };

